package com.findu.dto;

import java.util.LinkedList;

/**
 * 父母
 * @author ll
 *
 */
public class FUParent extends FUPerson {
	
	/**是否时家主*/
	private boolean isHouseholder = false;
	/**
	 * 所有的孩子
	 */
	private LinkedList<Long> childIds = null;
	
	public boolean isHouseholder() {
		return isHouseholder;
	}

	public void setHouseholder(boolean isHouseholder) {
		this.isHouseholder = isHouseholder;
	}
	
	public LinkedList<Long> getChildIds() {
		return childIds;
	}

	public void setChildIds(LinkedList<Long> childIds) {
		this.childIds = childIds;
	}	
}
